{
  "name": "@herbivore/api",
  "version": "2.0.0",
  "description": "Herbivore Password Manager - API",
  "author": "herbivore",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "tsc && cp package.json ./dist",
    "build:test": "tsc -p ./lib/spec/tsconfig.spec.json",
    "test": "NODE_OPTIONS=--loader=ts-node/esm jasmine --config=./lib/spec/jasmine.json",
    "publish:prod": "cp package.json ./dist && cd dist && npm publish --access public",
    "publish:dry": "cp package.json ./dist && cd dist && npm publish --access public --dry-run"
  },
  "engines": {
    "node": "^14.15.0 || >=16.10.0"
  },
  "dependencies": {
    "@herbivore/core": "file:../core/dist",
    "mongoose": "^6.5.0",
    "tslib": "^2.4.0",
    "js-base64": "^3.7.5"
  },
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^18.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "latest",
    "ts-jest": "latest",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "exports": {
    "./package.json": "./package.json",
    "./utils": {
      "types": "./utils/index.d.ts",
      "default": "./utils/index.js"
    },
    "./database": {
      "types": "./database/index.d.ts",
      "default": "./database/index.js"
    }
  },
  "keywords": [
    "api",
    "herbivore"
  ],
  "repository": {
    "url": "https://github.com/MultiDrew97/hpm-modules",
    "directory": "api",
    "type": "git"
  }
}
