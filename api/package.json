{
  "name": "@herbivore/api",
  "version": "2.0.0",
  "description": "Herbivore Password Manager - API",
  "author": "herbivore",
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "build:test": "tsc -p ./lib/spec/tsconfig.spec.json",
    "test": "NODE_OPTIONS=--loader=ts-node/esm jasmine --config=./lib/spec/jasmine.json",
    "publish:prod": "cp package.json ./dist && cd dist && npm publish --access public",
    "publish:dry": "cp package.json ./dist && cd dist && npm publish --access public --dry-run"
  },
  "engines": {
    "node": "^14.15.0 || >=16.10.0"
  },
  "dependencies": {
    "mongoose": "^6.5.0",
    "@herbivore/core": "file:../core/dist",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.6.4",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json",
    "./database": {
      "types": "./database/index.d.ts",
      "default": "./database/index.js"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "default": "./utils/index.js"
    },
    "./database/models": {
      "types": "./database/models/index.d.ts",
      "default": "./database/models/index.js"
    },
    "./database/models/user": {
      "types": "./database/models/user.d.ts",
      "default": "./database/models/user.js"
    },
    "./database/models/password": {
      "types": "./database/models/password.d.ts",
      "default": "./database/models/password.js"
    }
  },
  "keywords": [
    "api",
    "hpm",
    "password",
    "password manager"
  ],
  "repository" : {
    "url" : "https://github.com/MultiDrew97/hpm-modules",
    "directory" : "api",
    "type" : "git"
  }
}
